{
  "name": "francy-browser",
  "version": "0.9.0",
  "description": "Francy - An Interactive Discrete Mathematics Framework for GAP",
  "author": "Manuel Martins <manuelmachadomartins@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/mcmartins/francy.git"
  },
  "keywords": [
    "gap",
    "francy",
    "graphics",
    "interactivity"
  ],
  "license": "MIT",
  "scripts": {
    "docs": "./node_modules/.bin/esdoc",
    "lint": "./node_modules/.bin/eslint ./src/** ",
    "lint:fix": "./node_modules/.bin/eslint --fix ./src/**",
    "test": "./node_modules/.bin/karma start karma.conf.js",
    "coverage": "./node_modules/.bin/nyc --reporter=lcov --reporter=text-lcov npm run test",
    "webpack:develpment": "./node_modules/.bin/webpack --env.clean --progress",
    "webpack:production": "./node_modules/.bin/webpack --env.clean --env.production --progress",
    "webpack:all": "npm run webpack:develpment && ./node_modules/.bin/webpack --env.production --progress",
    "build:all": "npm run webpack:all && cp ./package.json ./*.md ./dist",
    "build": "npm run build:all"
  },
  "main": "index.js",
  "dependencies": {
    "d3": "5.5.0",
    "vis": "4.21.0",
    "francy": "0.9.0",
    "francy-renderer-d3": "0.9.0",
    "francy-renderer-vis": "0.9.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.6",
    "babel-loader": "7.1.5",
    "babel-plugin-transform-builtin-extend": "1.1.2",
    "babel-plugin-transform-decorators-legacy": "1.3.5",
    "babel-plugin-transform-es3-member-expression-literals": "6.22.0",
    "babel-plugin-transform-es3-property-literals": "6.22.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-es2015": "6.24.1",
    "chai": "4.1.2",
    "css-loader": "^1.0.0",
    "d3": "5.5.0",
    "del": "3.0.0",
    "esdoc": "1.1.0",
    "esdoc-standard-plugin": "1.0.0",
    "eslint": "5.2.0",
    "isparta": "4.1.0",
    "karma": "^3.0.0",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-mocha": "1.3.0",
    "karma-mocha-reporter": "2.2.5",
    "karma-webpack": "3.0.0",
    "lerna": "^3.0.6",
    "mocha": "5.2.0",
    "nyc": "12.0.2",
    "save-svg-as-png": "1.4.2",
    "seedrandom": "2.4.3",
    "style-loader": "^0.22.1",
    "webpack": "4.16.2",
    "webpack-cli": "3.1.0"
  },
  "files": [
    "README.md",
    "dist/"
  ]
}
